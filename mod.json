{
  "geode": "4.3.0",
  "gd": {
    "win": "2.2074",
    "android": "2.2074",
    "mac": "2.2074"
  },
  "version": "v1.0.0",
  "id": "lblazen.gdps_hub",
  "name": "GDPS Hub",
  "developers": ["GDPS Hub Team", "km7dev"],
  "description": "Preview private servers from GDPS Hub",
  "tags": ["online", "content"],
  "links": {
    "source": "https://github.com/kingminer7/gdps-hub",
    "community": "https://gdpshub.com/discord",
    "homepage": "https://gdpshub.com/"
  },
  "issues": {
    "info": "Report issues on the github issues page.",
    "url": "https://github.com/kingminer7/gdps-hub/issues"
  },
  "resources": {
    "spritesheets": {
      "gdpsHubSheet01": [
        "resources/sprites/gdps_hub.png",
        "resources/sprites/gdpsHubBtn.png",
        "resources/sprites/gdpsToolsBtn.png",
        "resources/sprites/info.png",
        "resources/sprites/preview.png"
      ]
    },
    "sprites": [
      "resources/sprites/bg.png",
      "resources/sprites/gdpshub-header.png",
      "resources/sprites/GH_square01.png"
    ]
  },
  "dependencies":
  {
    "km7dev.server_api": {
      "importance": "required",
      "version": "2.0.0"
    },
    "km7dev.gdps-switcher": {
      "importance": "recommended",
      "version": "1.3.0"
    }
  },
  "settings": {
    "hide-previewing": {
      "name": "Hide Previewing Label",
      "type": "bool",
      "default": false,
      "description": "Hides the 'Previewing' label when previewing a server."
    },
    "max-image": {
      "name": "Image Caching Max",
      "description": "Maximum images to cache at a time - 0 to not cache",
      "type": "int",
      "default": 20,
      "min": 0,
      "max": 100,
      "control": {
        "slider": true,
        "slider-step": 1,
        "input": true
      }
    }
  }
}
